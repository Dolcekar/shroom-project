<div class="wrapper container">
  <row class="row">
    <column class="col-md-12">
    <h2>I got 999 Mushrooms</h2>
    <ul id="mushroom-list" class="list-group">
      {{#each mushrooms}}
      <li data-id="{{this.id}}" class="list-group-item">
        <span>{{this}}, {{this.class}}</span>
      </li>
      {{/each}}
    </ul>
    </column>
  </row>
</div>
<script src="/js/index.js"></script>
